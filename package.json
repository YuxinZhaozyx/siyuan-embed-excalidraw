  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@types/node": "^20.2.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.2.0",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.62.1",
    "siyuan": "1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^6.2.1",
