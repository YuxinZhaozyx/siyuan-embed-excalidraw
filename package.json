  "homepage": "",
  "repository": "",
  "scripts": {
    "dev": "vite build --watch",
    "dev:app": "vite build --config vite.app.config.ts --mode=development",
    "release": "node ./release.js",
    "release:manual": "node ./release.js --mode=manual",
    "release:patch": "node ./release.js --mode=patch",
    "release:minor": "node ./release.js --mode=minor",
    "release:major": "node ./release.js --mode=major",
    "build:app": "vite build --config vite.app.config.ts",
    "build:plugin": "vite build",
    "build": "pnpm build:app && pnpm build:plugin"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
